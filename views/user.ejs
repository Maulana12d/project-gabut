<table>
    <thead>
        <tr>
            <th>Nama</th>
            <th>Email</th>
        </tr>
    </thead>

    <tbody>
        <% user.forEach(user) => { %> 
            <tr>
                <td><%= user.nama %></td>
                <td><%= user.email %></td>
            </tr>
        <% }) %>
    </tbody>
</table>